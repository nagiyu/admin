{
  "compilerOptions": {
    "target": "ES2018",
    "module": "commonjs",
    "lib": [
      "ES2018"
    ],
    "paths": {
      "@common/*": [
        "../typescript-common/common/*"
      ],
      "@admin/*": [
        "./*"
      ]
    },
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true
  }
}